{
  "name": "unpack-stream",
  "version": "1.0.2",
  "description": "Unpack a tarball stream",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "test": "npm run tsc && mos test README.md",
    "md": "mos README.md",
    "tsc": "rm -rf dist && tsc",
    "prepublish": "npm run tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zkochan/unpack-stream.git"
  },
  "keywords": [
    "tarball",
    "stream",
    "unpack"
  ],
  "author": {
    "name": "Zoltan Kochan",
    "email": "zoltan.kochan@gmail.com",
    "url": "http://kochan.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zkochan/unpack-stream/issues"
  },
  "homepage": "https://github.com/zkochan/unpack-stream#readme",
  "dependencies": {
    "@types/node": "^7.0.0",
    "decompress-maybe": "^1.0.0",
    "tar-fs": "^1.14.0"
  },
  "devDependencies": {
    "got": "^6.5.0",
    "mos": "^1.3.1",
    "mos-plugin-readme": "^1.0.4",
    "typescript": "^2.0.3"
  },
  "mos": {
    "plugins": [
      "readme"
    ],
    "installation": {
      "useShortAlias": true
    }
  },
  "engines": {
    "node": ">=4"
  }
}
