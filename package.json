{
  "name": "unpack-stream",
  "version": "3.0.2",
  "description": "Unpack a tarball stream",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "pretest": "rimraf test/dest",
    "test": "npm run tsc && preview && ts-node test --type-check",
    "md": "preview && mos README.md",
    "tsc": "rimraf dist && tsc",
    "prepublishOnly": "npm run tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zkochan/unpack-stream.git"
  },
  "keywords": [
    "tarball",
    "stream",
    "unpack"
  ],
  "author": {
    "name": "Zoltan Kochan",
    "email": "z@kochan.io",
    "url": "https://www.kochan.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zkochan/unpack-stream/issues"
  },
  "homepage": "https://github.com/zkochan/unpack-stream#readme",
  "dependencies": {
    "@types/node": "^10.0.0",
    "decompress-maybe": "^1.0.0",
    "ssri": "^5.0.0",
    "tar-fs": "^1.14.0"
  },
  "devDependencies": {
    "@types/tape": "^4.2.30",
    "got": "^8.0.0",
    "ignorable": "^1.0.1",
    "mos": "^2.0.0-alpha.3",
    "mos-plugin-readme": "^1.0.4",
    "package-preview": "^1.0.0",
    "rimraf": "^2.6.1",
    "tape": "^4.7.0",
    "ts-node": "^6.0.0",
    "typescript": "^2.4.2"
  },
  "mos": {
    "plugins": [
      "readme"
    ],
    "installation": {
      "useShortAlias": true
    }
  },
  "engines": {
    "node": ">=4"
  }
}
